<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>Galaxy Insurance</title>
    <link href="/css/index.css" rel="stylesheet" type="text/css">
  </head>
  <body>
    <%- include('partials/navbar.ejs') %>
    <%= message %>

    <div class="buttons">
      <button onClick = "redirect(event, '/auth/login')">Login</button>
      <button onClick = "redirect(event, '/quote/simulate')">Quote Generator</button>
      <button onClick = "redirect(event, '/about')">About Us</button>
      <button onClick = "redirect(event, '/contact')">Contact Us</button>
      <button onClick = "redirect(event, '/report_sinister')">REPORT A SINISTER</button>
    </div>

    <script>
      function redirect(e, url) {
        e.preventDefault();
        location.href = url;
      }
    </script>
  </body>
</html>